<DialogPane expanded="true" prefHeight="269.0" prefWidth="220.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
   <header>
      <AnchorPane prefHeight="48.0" prefWidth="220.0">
         <children>
            <Label layoutX="57.0" layoutY="14.0" text="AutoSave Settings" />
         </children>
      </AnchorPane>
   </header>
   <content>
      <GridPane alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Slider GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label ellipsisString="" text="AutoSave Interval (Minutes)" wrapText="true" GridPane.rowIndex="1" />
            <Label alignment="CENTER" contentDisplay="CENTER" ellipsisString="" text="Auto Save (Enable/Disable)" textAlignment="CENTER" wrapText="true" />
            <CheckBox ellipsisString="" mnemonicParsing="false" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </content>
   <graphic>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" />
   </graphic>
   <buttonTypes>
      <ButtonType fx:constant="CANCEL" />
      <ButtonType fx:constant="APPLY" />
   </buttonTypes>
</DialogPane>
